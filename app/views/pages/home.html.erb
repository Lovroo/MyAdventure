<% if user_signed_in? %>
  <div class="eleven" style="padding-top: 100px;color: wheat">
    <h1 id="naslovna"><b>Find your next destination!</b></h1>
  </div>
<div id='map' style='width: 100%; height: 700px;margin: auto;margin-bottom: 0px'>
</div>
    <%= javascript_pack_tag 'map'%>
  <% else %>
  <div class="eleven" style="padding-top: 200px;color: wheat">
